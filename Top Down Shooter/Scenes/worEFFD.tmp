script/source = "extends MeshInstance3D
var roach = preload(\"res://Editables/Widgets/Units/Roach.tscn\")
var spawn_period = 3.0
var spawn_period_cur = 0.0

func _process(delta):
	return
	spawn_period_cur += delta
	if multiplayer.is_server() and spawn_period_cur > spawn_period:
		spawn_period_cur = 0.0
		NetworkFactory.create_unit_at.rpc(
			NetworkFactory.swap_id_path(\"res://Editables/Widgets/Units/Roach.tscn\"),
			global_position,
			PlayerLib.PLAYER_THRUL.index
			)
"

